{
  "name": "badfunkstripe",
  "version": "1.0.0",
  "description": "A WordPress starter theme, based on underscores",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack --mode=production",
    "dev": "webpack --watch",
    "eslint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --quiet --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "wp-pot": "wp-pot --src '*.php' --dest-file 'languages/badfunkstripe.pot' --domain badfunkstripe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremypittard/badfunkstripe.git"
  },
  "keywords": [
    "wordpress",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "img-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "purgecss-webpack-plugin": "^1.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-sprite-loader": "^4.1.3",
    "svgo": "^1.1.1",
    "svgo-loader": "^2.2.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "wp-pot-cli": "^1.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
}
